<?xml version="1.0" encoding="UTF-8"?>
<netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2" enhance="false">
<aggregation type="union">
	{{#joins}}
	<netcdf>
	<aggregation type="joinExisting" dimName="time">
	    {{#files}}
			<netcdf location="{{.}}"/>
			{{/files}}
	</aggregation>
	</netcdf>
	{{/joins}}
</aggregation>	
</netcdf>
